// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  userId    String?
  name      String?
  nickname  String?
  email     String?
  email2    String?
  password  String?
  agreement Boolean?
  createdAt DateTime @default(now())
}
